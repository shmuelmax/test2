node {
          stage('Build'){
                        sh "echo Hello Jenkinsfile!!!"
          }
		  stage('deploy'){
						mvn clean install
						def server = Artifactory.server 'jfrog'
                        def uploadSpec = """{
  "files": [
    {
      "pattern": "/*froggy*.zip",
      "target": "bazinga-repo/froggy-files/"
    }
 ]
}"""
server.upload(uploadSpec)
	                     }
	}