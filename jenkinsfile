pipeline 
 {
    agent {'jenkins_slave'}
    tools { 
        maven 'mvn' 
        jdk 'jdk' 
    }
    stages {
        stage ('Initialize') {
			agent{
				label 'jenkins_slave'
			}
					steps {
						sh '''
							echo "PATH = ${PATH}"
							echo "M2_HOME = ${M2_HOME}"
						''' 
            }
        }

        stage ('Build') {
            steps {
                echo 'This is a minimal pipeline.'
				sh "mvn clean install"
            }
        }
    }

} 